<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="knightnews.android"
          android:versionCode="10"
          android:versionName="2.0.0">

    <uses-sdk
        android:minSdkVersion="9"
        android:targetSdkVersion="19"/>

    <uses-permission android:name="android.permission.INTERNET"/>
    <uses-permission android:name="android.permission.SEND_SMS"/>
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE"/>
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE"/>
    <uses-permission android:name="com.google.android.providers.gsf.permission.READ_GSERVICES"/>
    <!-- The following two permissions are not required to use
         Google Maps Android API v2, but are recommended. -->
    <uses-permission android:name="android.permission.ACCESS_COARSE_LOCATION"/>
    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION"/>

    <uses-feature
        android:glEsVersion="0x00020000"
        android:required="true"/>


    <application
        android:allowBackup="true"
        android:icon="@drawable/appicon"
        android:label="@string/app_name"
        android:theme="@style/CustomActionBarTheme">
        <activity
            android:name="knightnews.android.HomeActivity"
            android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity
            android:name="knightnews.android.ReaderActivity"
            android:label="@string/app_name"
            android:parentActivityName="knightnews.android.FeedPagerActivity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.sigmobile.ucf_news.FeedPagerActivity"/>
        </activity>
        <activity
            android:name="knightnews.android.FeedPagerActivity"
            android:label="@string/app_name"
            android:parentActivityName="knightnews.android.HomeActivity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.sigmobile.ucf_news.HomeActivity"/>
        </activity>
        <activity
            android:name="knightnews.android.EventsActivity"
            android:label="@string/app_name"
            android:parentActivityName="knightnews.android.HomeActivity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.sigmobile.ucf_news.HomeActivity"/>
        </activity>
        <activity
            android:name="knightnews.android.SportsActivity"
            android:label="@string/app_name"
            android:parentActivityName="knightnews.android.HomeActivity"
            android:configChanges="keyboardHidden|orientation|screenSize">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.sigmobile.ucf_news.HomeActivity"/>
        </activity>
        <activity
            android:name="knightnews.android.UcfMapActivity"
            android:label="@string/app_name"
            android:parentActivityName="knightnews.android.HomeActivity"
            android:configChanges="keyboardHidden|orientation|screenSize">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.sigmobile.ucf_news.HomeActivity"/>
        </activity>
        <activity
            android:name=".ReaderWebViewActivity"
            android:label="@string/app_name"
            android:parentActivityName="knightnews.android.FeedPagerActivity"
            android:configChanges="keyboardHidden|orientation|screenSize">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.sigmobile.ucf_news.FeedPagerActivity"/>
        </activity>
        <activity
            android:name="knightnews.android.FeedActivity"
            android:label="@string/app_name"></activity>

        <meta-data
            android:name="com.google.android.gms.version"
            android:value="@integer/google_play_services_version"/>
        <meta-data
            android:name="com.google.android.maps.v2.API_KEY"
            android:value="AIzaSyAKOWapT8ztymHqPUj7jx3CLtUg3SvLruU"/>

    </application>

</manifest>